#include <iostream>
#include <string>
#include <vector>

using std::string;
using std::cout;
using std::cin;
using std::endl;
using std::vector;

int main(){
	vector<int> a;
	for(int i = 0;i<10;i++)
		a.push_back(i);
	for(auto p=a.begin();p!=a.end();p++)
		*p=*p*2;
	for(auto p=a.begin();p!=a.end();p++)
		cout<<*p<<" ";
	return 0;
}
